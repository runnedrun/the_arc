{
  "name": "the_arc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build-functions": "WEBPACK_MODE='production' node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts",
    "check-types": "tsc --noEmit",
    "dev": "next dev",
    "emulate": "firebase emulators:start --project staging",
    "find:unused": "next-unused",
    "lint": "next lint",
    "start": "next start -p 8080",
    "test": "jest --watch",
    "use-demo": "source ./env_script/use-demo.sh",
    "watch-webpack": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts --watch",
    "profile-webpack": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts --profile --json > stats.json"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "babel-loader": "^9.2.1",
    "batch-promises": "^0.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "firebase": "^10.13.2",
    "firebase-admin": "^12.5.0",
    "firebase-functions": "^6.0.1",
    "lodash-es": "^4.17.21",
    "next": "14.2.13",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react": "^18",
    "react-dom": "^18",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "utility-types": "^3.11.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.5"
  },
  "entrypoints": [
    "app"
  ]
}
